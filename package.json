{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/app.ts",
  "scripts": {
    "build": "tsc",
    "start": "npm run build && NODE_ENV=production node -r module-alias/register ./dist/src/app.js",
    "dev": "NODE_ENV=development nodemon",
    "generate": "drizzle-kit generate:pg --config=drizzle.config.ts",
    "push": "ts-node ./src/db/migrate.ts",
    "drop": "drizzle-kit drop --config=drizzle.config.ts",
    "migrate": "drizzle-kit generate:pg --config=drizzle.config.ts && ts-node ./src/db/migrate.ts",
    "migrate:prod": "NODE_ENV=production drizzle-kit generate:pg --config=drizzle.config.ts && ts-node ./src/db/migrate.ts",
    "studio": "drizzle-kit studio"
  },
  "_moduleAliases": {
    "@": "./dist/src/"
  },
  "keywords": [
    "바퀴달린입",
    "연애토론",
    "만약에게임",
    "연애조언",
    "밸런스게임",
    "balance game",
    "if game"
  ],
  "author": "maruhxn",
  "license": "MIT",
  "dependencies": {
    "@types/passport-kakao": "^1.0.0",
    "bcrypt": "^5.1.0",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.28.2",
    "express": "^4.18.2",
    "helmet": "^7.0.0",
    "hpp": "^0.2.3",
    "jsonwebtoken": "^9.0.1",
    "module-alias": "^2.2.3",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "passport": "^0.6.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-kakao": "^1.0.1",
    "pm2": "^5.3.0",
    "postgres": "^3.3.5",
    "ulid": "^2.3.0",
    "winston": "^3.10.0",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@types/compression": "^1.7.2",
    "@types/express": "^4.17.17",
    "@types/hpp": "^0.2.2",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/morgan": "^1.9.4",
    "@types/multer": "^1.4.7",
    "@types/passport-google-oauth20": "^2.0.11",
    "@types/pg": "^8.10.2",
    "drizzle-kit": "^0.19.12",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.1.6"
  }
}
